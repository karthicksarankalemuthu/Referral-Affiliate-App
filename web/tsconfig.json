//@ts-nocheck
{
  "compilerOptions": {
    "target": "ESNext",                                  
    "module": "ESNext",                                  
    "moduleResolution": "node",                          
    "esModuleInterop": true,  
    "forceConsistentCasingInFileNames": true,  
    "noImplicitAny": true,                              
    "allowSyntheticDefaultImports": true,           
    "noEmit": true,                             
    "skipLibCheck": true,
    "alwaysStrict":false
                                  
  }
}
